{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "addons",
  "projects": {
    "imeepos-cli": {
      "root": "projects/imeepos-cli",
      "sourceRoot": "projects/imeepos-cli/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "iwe8:nestjs2",
          "options": {
            "tsConfig": "projects/imeepos-cli/tsconfig.lib.json"
          }
        }
      }
    },
    "iwe8": {
      "root": "projects/iwe8",
      "sourceRoot": "projects/iwe8/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "./dist/iwe8:nestjs2",
          "options": {
            "tsConfig": "projects/iwe8/tsconfig.lib.json",
            "watch": true
          }
        },
        "git": {
          "builder": "./dist/iwe8:git",
          "options": {}
        },
        "watch": {
          "builder": "./dist/iwe8:watch",
          "options": {
            "targets": [{
              "path": "projects/iwe8",
              "target": "iwe8:build"
            }, {
              "path": "projects/imeepos-cli",
              "target": "imeepos-cli:build"
            }]
          }
        }
      }
    },
    "default": {
      "root": "clients/default/",
      "sourceRoot": "clients/default/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build2": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/default",
            "index": "clients/default/src/index.html",
            "main": "clients/default/src/main.ts",
            "polyfills": "clients/default/src/polyfills.ts",
            "tsConfig": "clients/default/tsconfig.app.json",
            "assets": [
              "clients/default/src/favicon.ico",
              "clients/default/src/assets"
            ],
            "styles": [
              "clients/default/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "clients/default/src/environments/environment.ts",
                "with": "clients/default/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "build": {
          "builder": "./dist/iwe8:ng-builder",
          "options": {
            "ngTarget": "default:build2:production",
            "isDll": true,
            "name": "default"
          },
          "configurations": {
            "production": {
              "ngTarget": "default:build2:production",
              "isDll": false,
              "name": "default"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "default:build2"
          },
          "configurations": {
            "production": {
              "browserTarget": "default:build2:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "default:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "clients/default/src/test.ts",
            "polyfills": "clients/default/src/polyfills.ts",
            "tsConfig": "clients/default/tsconfig.spec.json",
            "karmaConfig": "clients/default/karma.conf.js",
            "styles": [
              "clients/default/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "clients/default/src/favicon.ico",
              "clients/default/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "clients/default/tsconfig.app.json",
              "clients/default/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "ngc": {
          "builder": "./dist/iwe8:ngc",
          "options": {
            "project": "clients/default/tsconfig.app.json"
          }
        },
        "pm2": {
          "builder": "./dist/iwe8:pm2",
          "options": {
            "name": "ngc",
            "script": "architect",
            "args": [
              "default:ngc",
              "--watch"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/default-server",
            "main": "clients/default/src/main.server.ts",
            "tsConfig": "clients/default/tsconfig.server.json"
          }
        },
        "app-shell": {
          "builder": "@angular-devkit/build-angular:app-shell",
          "options": {
            "browserTarget": "default:build",
            "serverTarget": "default:server",
            "route": "shell"
          }
        }
      }
    },
    "default-e2e": {
      "root": "clients/default-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "clients/default-e2e/protractor.conf.js",
            "devServerTarget": "default:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "default:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "clients/default-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ims-demo": {
      "root": "addons/ims-demo/",
      "sourceRoot": "addons/ims-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "targets": {
        "ng-addon": {
          "builder": "./dist/iwe8:ng-addon",
          "options": {
            "ngTarget": "ims-demo:build:production",
            "name": "ims_demo"
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ims-demo",
            "index": "addons/ims-demo/src/index.html",
            "main": "addons/ims-demo/src/app/app.module.ts",
            "tsConfig": "addons/ims-demo/tsconfig.app.json",
            "assets": [
              "addons/ims-demo/src/assets"
            ],
            "styles": [],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "addons/ims-demo/src/environments/environment.ts",
                "with": "addons/ims-demo/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ims-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ims-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ims-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "addons/ims-demo/src/test.ts",
            "polyfills": "addons/ims-demo/src/polyfills.ts",
            "tsConfig": "addons/ims-demo/tsconfig.spec.json",
            "karmaConfig": "addons/ims-demo/karma.conf.js",
            "styles": [
              "addons/ims-demo/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "addons/ims-demo/src/favicon.ico",
              "addons/ims-demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "addons/ims-demo/tsconfig.app.json",
              "addons/ims-demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ims-demo-e2e": {
      "root": "addons/ims-demo-e2e/",
      "projectType": "application",
      "targets": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "addons/ims-demo-e2e/protractor.conf.js",
            "devServerTarget": "ims-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ims-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "addons/ims-demo-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "default"
}
